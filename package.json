    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-plugin-svelte": "2.35.1",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "prettier-plugin-svelte": "3.2.6",
    "stylelint": "16.2.0",
    "stylelint-config-html": "1.1.0",
    "svelte": "4.2.9",
    "svelte-check": "3.6.3",
